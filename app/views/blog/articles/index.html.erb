<section>
  <ul class='no-bullets'>
  <% @articles.each do |article| %>
    <li class='article'>
      <span class='timestamp'>
        <%= render_timestamp(article.created_at) %>
      </span>
      <%= link_to article.title, article_path(article) %>
    </li>
  <% end %>
  </ul>
  <footer>
    <%= paginate @articles %>
  </footer>
</section>
